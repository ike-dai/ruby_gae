<html>
  <%= stylesheet_link_tag "application.css" %>
  <title>ユーザ一覧</title>
  <h1>登録ユーザ一覧</h1>
  <%= link_to('新規ユーザ登録',:controller => 'admin', :action => 'new') %>
  <br>
  <table class=table_list>
      <tr>
        <th>ユーザID</th>
        <th>名前</th>
      </tr>

    <% Users.all.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
      </tr>
    <% end %>
  </table>
</html>

